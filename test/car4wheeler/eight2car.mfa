package FourWheeler



sequence Choose {
	goto Eight2Car;
}

sequence Eight2Car {
	module.gradientInit();
	
	do {
		module.gradientPropagate();
	}	until (config.propagationTime);
	
	do {
		byte nr = (module.gradPri/2)*3;
		if (module.gradSec%2==0) {
			nr = nr + module.gradSec/2 + 1;
		}
		
		module.id = @Eight[nr];
	}
	
	do {retract(@Eight[0],0);
		retract(@Eight[3],4); 
	}
	
	do {	
		rotate(@Eight[3],-QUART);
		rotate(@Eight[4],-QUART);
	}
	
	extend(@Eight[4],2);
	
	do {rotate(@Eight[1],-QUART);
		retract(@Eight[6],2);}
	
	do {rotate(@Eight[4],QUART);
		rotate(@Eight[6],QUART); }
	
	extend(@Eight[0],0);
	retract(@Eight[6],6);
	
	do {rotate(@Eight[0],-QUART);
		rotate(@Eight[1],QUART);
	}
	
	rotate(@Eight[0],QUART);
	
	do {extend(@Eight[5],0);
		extend(@Eight[2],4);
		extend(@Eight[1],4);
	}
	
	retract(@Eight[4],2);
	retract(@Eight[3],6);
	
	do {rotate(@Eight[1],QUART);
		rotate(@Eight[3],QUART);
	}
	
	extend(@Eight[1],6);
	
	do {retract(@Eight[3],0);
		retract(@Eight[3],2);
	}
	rotate(@Eight[1],QUART);
	
	// goto Drive;
}

