	ID -> FuncRef{cons("FuncRef")}
	
	Obj "." ID -> MethodRef{cons("MethodRef")}
	
	ID -> VarRef{cons("VarRef")}
	
	
	
	
	StateGenericRef						-> StateRef{cons("StateGenericRef"),prefer}
	ID		 							-> StateRef{cons("StateRef")}
	
	
	
	context-free priorities
